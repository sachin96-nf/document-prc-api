				</on-error-propagate>
			</error-handler>
		</try>
		<logger level="INFO" doc:name="Exit Log"
			doc:id="cbfafdca-50b9-4466-bc74-e771f0b79140"
			message="File posted to IDP successfully" />
	</sub-flow>
	<sub-flow name="idp-get-attachment-subflow"
